{
    "name": "endroid/simple-excel",
    "description": "Endroid Simple Excel",
    "keywords": ["endroid", "sipmle", "excel"],
    "homepage": "https://github.com/endroid/SimpleExcel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "phpoffice/phpexcel": "~1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/var-dumper": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SimpleExcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SimpleExcel\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
